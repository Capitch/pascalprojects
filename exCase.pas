Program exCase ;

var tecla:char;

Begin

  write('Digite a tecla a, b ,c: ');
  read(tecla);
  
  case tecla of
       'a':begin
             writeln('Você pressionou a tecla (a)')
           end;
       'b':begin
			       writeln('Você pressionou a tecla (b)');
           end;
       'c':begin
			       writeln('Você pressionou a tecla (c)');
           end;    
  end;
	
	readln;
	         
End.
